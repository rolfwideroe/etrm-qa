				<CurrencyAccount CurrencyIsoCode="USD" BalanceDate="0001-01-01" NominalAmount="6666.66" UpdatedBy=""/>
				<CurrencyAccount CurrencyIsoCode="NOK" BalanceDate="0001-01-01" NominalAmount="-7777.77" UpdatedBy=""/>
			</CurrencyAccounts>
		</Insert>
		<Insert InsertBalanceDate="2011-11-01">
