  <ExpectedResult Description="Test inital data for ERM Regresssion">
    <GetMethod>
      <MostRecent>false</MostRecent>
      <DateRangeCriteria>
        <BalanceDateFrom>2011-10-01</BalanceDateFrom>
        <BalanceDateTo>2011-11-10</BalanceDateTo>
      </DateRangeCriteria>
    </GetMethod>
    <CurrencyAccounts>
		<CurrencyAccount CurrencyIsoCode="NOK" BalanceDate="2011-10-01" NominalAmount="-7777.77" UpdatedBy="DealImport"/>
		<CurrencyAccount CurrencyIsoCode="NOK" BalanceDate="2011-11-01" NominalAmount="222222" UpdatedBy="DealImport"/>
		<CurrencyAccount CurrencyIsoCode="NOK" BalanceDate="2011-11-10" NominalAmount="-4444444.444" UpdatedBy="DealImport"/>
		<CurrencyAccount CurrencyIsoCode="USD" BalanceDate="2011-10-01" NominalAmount="6666.66" UpdatedBy="DealImport"/>
		<CurrencyAccount CurrencyIsoCode="USD" BalanceDate="2011-11-01" NominalAmount="111111" UpdatedBy="DealImport"/>
		<CurrencyAccount CurrencyIsoCode="USD" BalanceDate="2011-11-10" NominalAmount="-3333333.333" UpdatedBy="DealImport"/>
	  </CurrencyAccounts>
  </ExpectedResult>
</ExpectedResults>
